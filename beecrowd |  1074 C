#include<stdio.h>

int main()
{
    int X,N,a;
    scanf("%d", &N);
    
    for (a=1;a<=N;a++){
        
    scanf ("%d",&X);
    
    if((X%2==0)&&(X>0)){
    printf("EVEN POSITIVE\n");
}
    else if((X%2==1)&&(X>0)){
    printf("ODD POSITIVE\n");
}
    if(X==0){
    printf("NULL\n");
}
    else if((X%2==0)&&(X<0)) {
    printf("EVEN NEGATIVE\n");
 }
    else if((X%2==-1)&&(X<0)){
    printf("ODD NEGATIVE\n");
    }
    }
    return 0;
}
