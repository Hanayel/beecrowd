#include <stdio.h>

    int main() {

        float n1, n2,n3;
        double soma;
        float media;

        scanf("%f", & n1);
        scanf("%f", & n2);
        scanf("%f", & n3);

        (soma = (n1*2+n2*3+n3*5));
        (media= soma /(2+3+5));

        printf("MEDIA = %.1f\n", media);

    return 0;
}
