// tarefa 1 estacas. O site n aceitou. foi usado o algoritmo de Euclides como base

#include <stdio.h>
int mdc(int a, int b){
    while (a != b){
        if (a > b)
            a = a - b;
        else
            b = b - a;

        if (b == 0)

    return a;
    
        else
            
      return mdc(b, a % b);

    return a;
	}
}

    int main() {
    int x, y;
    while (scanf("%d %d", &x, &y) != EOF) {
        printf("%d\n", (x + x + y + y) / mdc(x, y));

    }
    return 0;
}

